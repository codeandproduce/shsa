		}

		if($('body').width() <= 400){
			$('.nav-logo').css('margin-top','11px');
		}else{
			$('.nav-logo').css('margin-top','25px');
		}
	});
	$(window).resize(function(){
		if($('body').width() < 650){
			$('.nav-logo img').attr('src', '/img/smalllogonav.png');
		}else{
			$('.nav-logo img').attr('src', '/img/logonav.png');
		}

		if($('body').width() <= 400){
			$('.nav-logo').css('margin-top','11px');
		}else{
			$('.nav-logo').css('margin-top','25px');
		}
	});
	</script>
